/**app.wxss**/
.container {
  /* height: 100%; */
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
view,text{
   box-sizing: border-box;
   font-family: '微软雅黑','Microsoft YaHei';
}
.flex{
  display: flex;
  -webkit-display: flex;
}
.fn-left{
  float: left;
}
.fn-right{
  float: right;
}
.clearfix:before, .clearfix:after {
    content: " ";
    display: table
}
.clearfix:after {
    clear: both;
}
.fn-text-overflow {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}